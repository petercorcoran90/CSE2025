package weekOne;

public class HelloWorld {

	public static void main(String[] args) {
		//identifiers
		String theNameOfThePerson="Peter";
		String theAddress = "Roscommon";
		String favouriteMovie = "";
		int age = 34;
		
		System.out.println(theNameOfThePerson);
		System.out.println(theAddress);
		System.out.println(favouriteMovie);
		System.out.println(age);
	}

}
